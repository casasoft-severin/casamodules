{
    "name": "casasoft/casamodules",
    "type": "library",
    "description": "Casasoft Modules for Zend Framework 2 (PSR7) implementations.",
    "keywords": [
        "zf2"
    ],
    "homepage": "http://casasoft.ch/",
    "license": "GPL",
    "authors": [
        {
            "name": "Jens Stalder",
            "email": "jens.stalder@casasoft.ch"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "laminas/laminas-dependency-plugin": "^2.1"
    },
    "require-dev": {},
    "suggest": {},
    "autoload": {
        "psr-0": {
            "CasaLogService\\": "src/",
            "CasasoftAuth\\": "src/",
            "CasasoftEmail\\": "src/",
            "CasasoftHelpers\\": "src/",
            "CasasoftThumb\\": "src/",
            "CasasoftStandards\\": "src/",
            "DbGenerator\\": "src/",
            "CasasoftMessenger\\": "src/",
            "CasasoftGeo\\": "src/"
        },
        "classmap": [
            "src/CasaLogService/src/CasaLogService/Service/LogService.php"
        ]
    },
    "config": {},
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}